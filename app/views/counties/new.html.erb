<h1>New county</h1>

<% form_for(@county) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :state_id %><br/>
    <%= f.collection_select :state_id, @states, :id, :name %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', counties_path %>
